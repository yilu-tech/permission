{
    "name": "yilu-tech/permission",
    "description": "Laravel Permission.",
    "keywords": [
        "laravel",
        "permission"
    ],
    "license": "MIT",
    "support": {
    },
    "authors": [
        {
            "name": "yj",
            "email": "yj@yilu.co"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0",
        "laravel/framework": "^5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.4"
    },
    "autoload": {
        "psr-4": {
            "YiluTech\\Permission\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YiluTech\\Permission\\PermissionServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
